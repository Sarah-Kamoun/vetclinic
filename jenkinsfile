pipeline{

    agent any

    stages {

        stage('Git Checkout'){

            steps{

                script{

                    git branch: 'main', url: 'https://github.com/Sarah-Kamoun/vetclinic.git'
                }
            }
        }
        stage('UNIT testing'){

            steps{

                script{

                    bat 'mvn test'
                }
            }
        }

    }
}